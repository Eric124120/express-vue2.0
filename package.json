{
  "name": "vue2-test",
  "description": "A Vue.js project",
  "author": "huangchengwen",
  "private": true,
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "build-w": "cross-env NODE_ENV=production webpack -w --progress --hide-modules",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --open --inline --hot --port 3000",
    "mock": "node_modules/.bin/json-server --watch mock/db.json --routes mock/routes.json --port 9090",
    "mockdev": "npm run mock & npm run dev",
    "server-dev": "node-dev ./bin/www",
    "supervisor2": "supervisor ./app2 & npm run build-w"
  },
